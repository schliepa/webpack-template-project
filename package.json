{
	"name": "webpack-template-project",
	"version": "0.0.1",
	"author": "Markus Stadler",
	"license": "MIT",
	"description": "Generic Project Webpack Template",
	"keywords": [],
	"type": "module",
	"main": "index.js",
	"scripts": {
		"start": "webpack --watch --config webpack.config.js",
		"build": "webpack --env production --config webpack.config.js",
		"build:debug": "webpack --env development",
		"lint": "eslint src/**",
		"lint-fix": "eslint --fix src/**",
		"clean": "rm -rf dist"
	},
	"types": "dist/types/**/*.d.ts",
	"files": [
		"dist/*.js*",
		"dist/*.d.ts",
		"dist/types/**/*.d.ts"
	],
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@stylistic/eslint-plugin": "2.13.0",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"babel-loader": "^9.2.1",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"eslint": "^9.18.0",
		"eslint-webpack-plugin": "^4.2.0",
		"fork-ts-checker-notifier-webpack-plugin": "^9.0.0",
		"fork-ts-checker-webpack-plugin": "^9.0.2",
		"html-loader": "5.1.0",
		"html-webpack-plugin": "^5.6.0",
		"ifdef-loader": "^2.3.2",
		"terser-webpack-plugin": "^5.3.11",
		"ts-loader": "^9.5.2",
		"typescript": "^5.7.3",
		"webpack": "^5.97.1",
		"webpack-cli": "^6.0.1"
